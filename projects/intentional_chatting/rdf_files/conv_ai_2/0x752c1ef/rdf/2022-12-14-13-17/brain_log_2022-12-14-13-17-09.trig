            grasp:Statement ;
        rdfs:label "chat110_utterance3_char0-20" ;
        gaf:containsDenotation leolaniWorld:music,
            leolaniWorld:participant1 ;
        gaf:denotes leolaniWorld:participant1_prefer_music ;
        grasp:hasAttribution leolaniTalk:participant1_prefer_music_0000 ;
