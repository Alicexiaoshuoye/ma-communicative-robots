leolaniWorld:Instances {
    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:pizza a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat91_utterance0_char0-33 .

    leolaniWorld:speaker1 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat91_utterance0_char0-33 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat91_utterance0_char0-33 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat91_utterance0_char0-33" ;
        gaf:containsDenotation leolaniWorld:pizza,
            leolaniWorld:speaker1 ;
        gaf:denotes leolaniWorld:speaker1_like_pizza ;
        grasp:hasAttribution leolaniTalk:speaker1_like_pizza_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:41:22.914119"^^xml1:string ;
        rdf:value "How do you like the pizza here ? "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat91_utterance0 .

    leolaniTalk:speaker1_like_pizza_0000 a grasp:Attribution ;
        rdfs:label "speaker1_like_pizza_0000" ;
        grasp:isAttributionFor leolaniTalk:chat91_utterance0_char0-33 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker2" .

    leolaniWorld:speaker1_like_pizza a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker1_like_pizza" ;
        gaf:denotedBy leolaniTalk:chat91_utterance0_char0-33 .

    leolaniWorld:speaker2 owl:sameAs leolaniWorld:speaker2 .
}

leolaniTalk:Interactions {
    leolaniContext:context21 a eps:Context ;
        rdfs:label "context21" ;
        n2mu:id "21"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-12 ;
        sem:hasPlace leolaniContext:unknown21 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat91 .

    leolaniContext:2022-12-12 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-12" ;
        time:day "12"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown21 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "244"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat91 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat91" ;
        n2mu:id "91"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat91_utterance0 .

    leolaniTalk:chat91_utterance0 a grasp:Utterance,
