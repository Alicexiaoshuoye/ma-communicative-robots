leolaniTalk:Interactions {
    leolaniContext:context20 a eps:Context ;
        rdfs:label "context20" ;
        n2mu:id "20"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-14 ;
        sem:hasPlace leolaniContext:unknown20 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat212 .

    leolaniContext:2022-12-14 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-14" ;
        time:day "14"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown20 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "46"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat212 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat212" ;
        n2mu:id "212"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat212_utterance1 .

    leolaniTalk:chat212_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat212_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker1 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker1" .
}

leolaniTalk:Perspectives {
    leolaniTalk:Ripen_hit_run_0000 a grasp:Attribution ;
        rdfs:label "Ripen_hit_run_0000" ;
        grasp:isAttributionFor leolaniTalk:chat212_utterance1_char0-40 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    leolaniTalk:chat212_utterance1_char0-40 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat212_utterance1_char0-40" ;
        gaf:containsDenotation leolaniWorld:Ripen,
            leolaniWorld:run ;
        gaf:denotes leolaniWorld:Ripen_hit_run ;
        grasp:hasAttribution leolaniTalk:Ripen_hit_run_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker1 ;
        sem:hasBeginTimeStamp "2022-12-14 11:42:04.203974"^^xml1:string,
            "2022-12-14 11:42:04.732344"^^xml1:string,
            "2022-12-14 11:42:05.242321"^^xml1:string,
            "2022-12-14 11:42:05.753352"^^xml1:string,
            "2022-12-14 11:42:06.356251"^^xml1:string,
            "2022-12-14 11:42:06.909573"^^xml1:string ;
        rdf:value " Yeah , Cal Ripen just hit a home run . "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat212_utterance1 .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:leolani_know_speaker1 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker1 .
}

leolaniWorld:Instances {
    leolaniWorld:Ripen a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat212_utterance1_char0-40 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:run a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat212_utterance1_char0-40 .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance .
}

leolaniWorld:Ripen_hit_run {
    leolaniWorld:Ripen n2mu:hit leolaniWorld:run .
}

leolaniWorld:Claims {
    leolaniWorld:Ripen_hit_run a gaf:Assertion,
            sem:Event ;
        rdfs:label "Ripen_hit_run" ;
        gaf:denotedBy leolaniTalk:chat212_utterance1_char0-40 .

    leolaniWorld:leolani_know_speaker1 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker1" .

    leolaniWorld:speaker1 owl:sameAs leolaniWorld:speaker1 .
}

