leolaniWorld:Instances {
    leolaniWorld:Ripen a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat122_utterance1_char0-40 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:run a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat122_utterance1_char0-40 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance .
}

leolaniTalk:Perspectives {
    leolaniTalk:Ripen_hit_run_0000 a grasp:Attribution ;
        rdfs:label "Ripen_hit_run_0000" ;
        grasp:isAttributionFor leolaniTalk:chat122_utterance1_char0-40 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    leolaniTalk:chat122_utterance1_char0-40 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat122_utterance1_char0-40" ;
        gaf:containsDenotation leolaniWorld:Ripen,
            leolaniWorld:run ;
        gaf:denotes leolaniWorld:Ripen_hit_run ;
        grasp:hasAttribution leolaniTalk:Ripen_hit_run_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:41:17.918551"^^xml1:string,
            "2022-12-12 18:41:18.435560"^^xml1:string,
            "2022-12-12 18:41:18.904257"^^xml1:string,
            "2022-12-12 18:41:19.358984"^^xml1:string,
            "2022-12-12 18:41:19.829915"^^xml1:string ;
        rdf:value " Yeah , Cal Ripen just hit a home run . "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat122_utterance1 .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:Claims {
    leolaniWorld:Ripen_hit_run a gaf:Assertion,
            sem:Event ;
        rdfs:label "Ripen_hit_run" ;
        gaf:denotedBy leolaniTalk:chat122_utterance1_char0-40 .

    leolaniWorld:leolani_know_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker2" .

    leolaniWorld:speaker2 owl:sameAs leolaniWorld:speaker2 .
}

leolaniTalk:Interactions {
    leolaniContext:context20 a eps:Context ;
        rdfs:label "context20" ;
        n2mu:id "20"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-12 ;
        sem:hasPlace leolaniContext:unknown20 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat122 .

    leolaniContext:2022-12-12 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-12" ;
        time:day "12"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown20 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "186"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat122 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat122" ;
        n2mu:id "122"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat122_utterance1 .

    leolaniTalk:chat122_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat122_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker2 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker2" .
}

leolaniWorld:leolani_know_speaker2 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker2 .
}

leolaniWorld:Ripen_hit_run {
    leolaniWorld:Ripen n2mu:hit leolaniWorld:run .
}

