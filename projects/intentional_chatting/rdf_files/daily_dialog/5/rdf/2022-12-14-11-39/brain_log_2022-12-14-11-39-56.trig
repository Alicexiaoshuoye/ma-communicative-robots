        gaf:denotedIn leolaniTalk:chat41_utterance1_char0-187 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:month a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat41_utterance1_char0-187 .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance .

    leolaniWorld:speaker2 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat41_utterance1_char0-187 .
}

leolaniWorld:speaker2_look-at_horoscope {
    leolaniWorld:speaker2 n2mu:look-at leolaniWorld:horoscope .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker1 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker1" .

    leolaniWorld:speaker2_look-at_horoscope a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker2_look-at_horoscope" ;
