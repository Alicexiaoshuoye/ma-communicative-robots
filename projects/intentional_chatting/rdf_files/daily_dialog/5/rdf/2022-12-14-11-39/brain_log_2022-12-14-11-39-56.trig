    leolaniWorld:speaker2_look-for_month a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker2_look-for_month" ;
        gaf:denotedBy leolaniTalk:chat41_utterance1_char0-187 .

    leolaniWorld:speaker1 owl:sameAs leolaniWorld:speaker1 .
}

leolaniWorld:speaker2_look-for_month {
