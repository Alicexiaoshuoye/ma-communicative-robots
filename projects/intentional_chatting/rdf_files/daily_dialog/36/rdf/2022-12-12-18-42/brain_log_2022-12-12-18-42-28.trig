@prefix eps: <http://cltl.nl/episodicawareness/> .
@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix time: <http://www.w3.org/TR/owl-time/#> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:speaker1 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat11_utterance0_char0-21 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat11_utterance0_char0-21 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat11_utterance0_char0-21 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat11_utterance0_char0-21" ;
        gaf:containsDenotation leolaniWorld:speaker1,
            leolaniWorld:speaker2 ;
        gaf:denotes leolaniWorld:speaker2_help_speaker1 ;
        grasp:hasAttribution leolaniTalk:speaker2_help_speaker1_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:42:26.329063"^^xml1:string,
            "2022-12-12 18:42:27.012117"^^xml1:string,
            "2022-12-12 18:42:27.691295"^^xml1:string,
            "2022-12-12 18:42:28.402123"^^xml1:string ;
        rdf:value "How may I help you ? "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat11_utterance0 .

    leolaniTalk:speaker2_help_speaker1_0000 a grasp:Attribution ;
        rdfs:label "speaker2_help_speaker1_0000" ;
        grasp:isAttributionFor leolaniTalk:chat11_utterance0_char0-21 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker2" .

    leolaniWorld:speaker2_help_speaker1 a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker2_help_speaker1" ;
        gaf:denotedBy leolaniTalk:chat11_utterance0_char0-21 .

    leolaniWorld:speaker2 owl:sameAs leolaniWorld:speaker2 .
}

leolaniTalk:Interactions {
    leolaniContext:context36 a eps:Context ;
        rdfs:label "context36" ;
        n2mu:id "36"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-12 ;
        sem:hasPlace leolaniContext:unknown36 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat11 .

    leolaniContext:2022-12-12 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-12" ;
        time:day "12"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown36 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "202"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat11 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat11" ;
        n2mu:id "11"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat11_utterance0 .

    leolaniTalk:chat11_utterance0 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat11_utterance0" ;
        n2mu:id "0"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker2 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker2" .
}

leolaniWorld:leolani_know_speaker2 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker2 .
}

leolaniWorld:speaker2_help_speaker1 {
    leolaniWorld:speaker2 n2mu:help leolaniWorld:speaker1 .
}

