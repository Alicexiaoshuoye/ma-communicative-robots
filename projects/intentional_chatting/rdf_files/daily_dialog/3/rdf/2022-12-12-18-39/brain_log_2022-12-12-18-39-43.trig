leolaniWorld:Instances {
    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:right a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat34_utterance0_char0-20 .

    leolaniWorld:speaker1 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat34_utterance0_char0-20,
            leolaniTalk:chat34_utterance1_char0-85 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance .

    leolaniWorld:terrify a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat34_utterance1_char0-85 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat34_utterance0_char0-20 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat34_utterance0_char0-20" ;
        gaf:containsDenotation leolaniWorld:right,
            leolaniWorld:speaker1 ;
        gaf:denotes leolaniWorld:speaker1_be_right ;
        grasp:hasAttribution leolaniTalk:speaker1_be_right_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:39:41.142510"^^xml1:string,
            "2022-12-12 18:39:41.719693"^^xml1:string,
            "2022-12-12 18:39:42.758440"^^xml1:string,
            "2022-12-12 18:39:43.768361"^^xml1:string ;
        rdf:value "Are you all right ? "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat34_utterance0 .

    leolaniTalk:chat34_utterance1_char0-85 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat34_utterance1_char0-85" ;
        gaf:containsDenotation leolaniWorld:speaker1,
            leolaniWorld:terrify ;
        gaf:denotes leolaniWorld:speaker1_be_terrify ;
        grasp:hasAttribution leolaniTalk:speaker1_be_terrify_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:39:42.105183"^^xml1:string,
            "2022-12-12 18:39:43.135449"^^xml1:string ;
        rdf:value " I will be all right soon . I was terrified when I watched them fall from the wire . "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat34_utterance1 .

    leolaniTalk:speaker1_be_right_0000 a grasp:Attribution ;
        rdfs:label "speaker1_be_right_0000" ;
        grasp:isAttributionFor leolaniTalk:chat34_utterance0_char0-20 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    leolaniTalk:speaker1_be_terrify_0000 a grasp:Attribution ;
        rdfs:label "speaker1_be_terrify_0000" ;
        grasp:isAttributionFor leolaniTalk:chat34_utterance1_char0-85 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker2" .

    leolaniWorld:speaker1_be_right a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker1_be_right" ;
        gaf:denotedBy leolaniTalk:chat34_utterance0_char0-20 .

    leolaniWorld:speaker1_be_terrify a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker1_be_terrify" ;
        gaf:denotedBy leolaniTalk:chat34_utterance1_char0-85 .

    leolaniWorld:speaker2 owl:sameAs leolaniWorld:speaker2 .
}

leolaniWorld:speaker1_be_right {
    leolaniWorld:speaker1 n2mu:be leolaniWorld:right .
}

leolaniTalk:Interactions {
    leolaniContext:context3 a eps:Context ;
        rdfs:label "context3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-12 ;
        sem:hasPlace leolaniContext:unknown3 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat34 .

    leolaniContext:2022-12-12 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-12" ;
        time:day "12"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown3 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "198"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat34 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat34" ;
        n2mu:id "34"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat34_utterance0,
            leolaniTalk:chat34_utterance1 .

    leolaniTalk:chat34_utterance0 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat34_utterance0" ;
        n2mu:id "0"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker2 .

    leolaniTalk:chat34_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat34_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker2 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker2" .
}

leolaniWorld:leolani_know_speaker2 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker2 .
}

leolaniWorld:speaker1_be_terrify {
    leolaniWorld:speaker1 n2mu:be leolaniWorld:terrify .
}

