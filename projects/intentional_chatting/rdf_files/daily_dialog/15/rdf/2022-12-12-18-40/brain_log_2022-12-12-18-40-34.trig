@prefix eps: <http://cltl.nl/episodicawareness/> .
@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix time: <http://www.w3.org/TR/owl-time/#> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:Instances {
    leolaniWorld:grandma a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat70_utterance0_char0-41 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat70_utterance0_char0-41 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat70_utterance0_char0-41 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat70_utterance0_char0-41" ;
        gaf:containsDenotation leolaniWorld:grandma,
            leolaniWorld:speaker2 ;
        gaf:denotes leolaniWorld:speaker2_see_grandma ;
        grasp:hasAttribution leolaniTalk:speaker2_see_grandma_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:40:34.085115"^^xml1:string,
            "2022-12-12 18:40:34.674679"^^xml1:string ;
        rdf:value "Hi Bill , I saw your grandma yesterday . "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat70_utterance0 .

    leolaniTalk:speaker2_see_grandma_0000 a grasp:Attribution ;
        rdfs:label "speaker2_see_grandma_0000" ;
        grasp:isAttributionFor leolaniTalk:chat70_utterance0_char0-41 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker2" .

    leolaniWorld:speaker2_see_grandma a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker2_see_grandma" ;
        gaf:denotedBy leolaniTalk:chat70_utterance0_char0-41 .

    leolaniWorld:speaker2 owl:sameAs leolaniWorld:speaker2 .
}

leolaniTalk:Interactions {
    leolaniContext:context15 a eps:Context ;
        rdfs:label "context15" ;
        n2mu:id "15"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-12 ;
        sem:hasPlace leolaniContext:unknown15 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat70 .

    leolaniContext:2022-12-12 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-12" ;
        time:day "12"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown15 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "100"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat70 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat70" ;
        n2mu:id "70"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat70_utterance0 .

    leolaniTalk:chat70_utterance0 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat70_utterance0" ;
        n2mu:id "0"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker2 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker2" .
}

leolaniWorld:leolani_know_speaker2 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker2 .
}

leolaniWorld:speaker2_see_grandma {
    leolaniWorld:speaker2 n2mu:see leolaniWorld:grandma .
}

