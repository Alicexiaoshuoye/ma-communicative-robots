leolaniWorld:Instances {
    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:speaker1 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat195_utterance3_char0-33 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat195_utterance3_char0-33 .
}

leolaniTalk:Interactions {
    leolaniContext:context42 a eps:Context ;
        rdfs:label "context42" ;
        n2mu:id "42"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-14 ;
        sem:hasPlace leolaniContext:unknown42 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat195 .

    leolaniContext:2022-12-14 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-14" ;
        time:day "14"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown42 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "166"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat195 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat195" ;
        n2mu:id "195"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat195_utterance3 .

    leolaniTalk:chat195_utterance3 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat195_utterance3" ;
        n2mu:id "3"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker2 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker2" .
}

leolaniWorld:leolani_know_speaker2 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker2 .
}

leolaniWorld:speaker1_meet_speaker2 {
    leolaniWorld:speaker1 n2mu:meet leolaniWorld:speaker2 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat195_utterance3_char0-33 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat195_utterance3_char0-33" ;
        gaf:containsDenotation leolaniWorld:speaker1,
            leolaniWorld:speaker2 ;
        gaf:denotes leolaniWorld:speaker1_meet_speaker2 ;
        grasp:hasAttribution leolaniTalk:speaker1_meet_speaker2_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-14 11:25:14.528502"^^xml1:string,
            "2022-12-14 11:25:15.730810"^^xml1:string,
            "2022-12-14 11:25:16.791055"^^xml1:string,
            "2022-12-14 11:25:17.848656"^^xml1:string ;
        rdf:value " Sure . Where shall I meet you ? "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat195_utterance3 .

    leolaniTalk:speaker1_meet_speaker2_0000 a grasp:Attribution ;
        rdfs:label "speaker1_meet_speaker2_0000" ;
        grasp:isAttributionFor leolaniTalk:chat195_utterance3_char0-33 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker2" .

    leolaniWorld:speaker1_meet_speaker2 a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker1_meet_speaker2" ;
        gaf:denotedBy leolaniTalk:chat195_utterance3_char0-33 .

    leolaniWorld:speaker2 owl:sameAs leolaniWorld:speaker2 .
}

