        rdfs:label "leolani" .

    leolaniWorld:speaker1 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat193_utterance2_char0-42,
            leolaniTalk:chat193_utterance3_char0-40 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance .

    leolaniWorld:storm a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat193_utterance2_char0-42 .

    leolaniWorld:sweat a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat193_utterance3_char0-40 .
}

leolaniWorld:speaker1_work_sweat {
