        grasp:wasAttributedTo leolaniWorld:speaker1 ;
        sem:hasBeginTimeStamp "2022-12-14 11:48:01.047416"^^xml1:string ;
        rdf:value " I agree . You can't expect a close relationship with a dishonest person . Those are the people that will turn their back on you when you are in need . "^^xml1:string ;
