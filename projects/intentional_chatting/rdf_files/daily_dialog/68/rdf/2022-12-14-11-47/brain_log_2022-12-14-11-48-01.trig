            "2022-12-14 11:47:59.723746"^^xml1:string,
            "2022-12-14 11:48:01.494814"^^xml1:string ;
        rdf:value " Just so so . Last night I met this great guy at the bar and we had such a wonderful time , so we decided to meet again tonight . "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat174_utterance1 .

    leolaniTalk:chat174_utterance3_char0-317 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat174_utterance3_char0-317" ;
        gaf:containsDenotation leolaniWorld:feeling,
            leolaniWorld:speaker2 ;
        gaf:denotes leolaniWorld:speaker2_have_feeling ;
