    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance .

    leolaniWorld:tan a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat100_utterance0_char0-30 .
}

leolaniWorld:speaker1_look_tan {
