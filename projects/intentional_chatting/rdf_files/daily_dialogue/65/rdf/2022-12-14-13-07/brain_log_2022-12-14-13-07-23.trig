@prefix eps: <http://cltl.nl/episodicawareness/> .
@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix graspe: <http://groundedannotationframework.org/grasp/emotion#> .
@prefix graspf: <http://groundedannotationframework.org/grasp/factuality#> .
@prefix grasps: <http://groundedannotationframework.org/grasp/sentiment#> .
@prefix leolaniContext: <http://cltl.nl/leolani/context/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix time: <http://www.w3.org/TR/owl-time/#> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniTalk:Interactions {
    leolaniContext:context65 a eps:Context ;
        rdfs:label "context65" ;
        n2mu:id "65"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-14 ;
        sem:hasPlace leolaniContext:unknown65 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat150 .

    leolaniContext:2022-12-14 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-14" ;
        time:day "14"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown65 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "142"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat150 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat150" ;
        n2mu:id "150"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat150_utterance2 .

    leolaniTalk:chat150_utterance2 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat150_utterance2" ;
        n2mu:id "2"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker1 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker1" .
}

leolaniWorld:speaker2_take_material {
    leolaniWorld:speaker2 n2mu:take leolaniWorld:material .
}

leolaniWorld:leolani_know_speaker1 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker1 .
}

leolaniWorld:Instances {
    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:material a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat150_utterance2_char0-60 .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance .

    leolaniWorld:speaker2 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat150_utterance2_char0-60 .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker1 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker1" .

    leolaniWorld:speaker2_take_material a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker2_take_material" ;
        gaf:denotedBy leolaniTalk:chat150_utterance2_char0-60 .

    leolaniWorld:speaker1 owl:sameAs leolaniWorld:speaker1 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat150_utterance2_char0-60 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat150_utterance2_char0-60" ;
        gaf:containsDenotation leolaniWorld:material,
            leolaniWorld:speaker2 ;
        gaf:denotes leolaniWorld:speaker2_take_material ;
        grasp:hasAttribution leolaniTalk:speaker2_take_material_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker1 ;
        sem:hasBeginTimeStamp "2022-12-14 13:07:22.560284"^^xml1:string,
            "2022-12-14 13:07:23.575243"^^xml1:string ;
        rdf:value " Will you take my materials back from him for me ? Please ! "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat150_utterance2 .

    leolaniTalk:speaker2_take_material_0000 a grasp:Attribution ;
        rdfs:label "speaker2_take_material_0000" ;
        grasp:isAttributionFor leolaniTalk:chat150_utterance2_char0-60 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

