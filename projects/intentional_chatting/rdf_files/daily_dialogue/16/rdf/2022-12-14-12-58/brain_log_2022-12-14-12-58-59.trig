@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .

leolaniWorld:speaker1_like_class {
    leolaniWorld:speaker1 n2mu:like leolaniWorld:class .
}

leolaniTalk:Interactions {
    leolaniContext:context16 a eps:Context ;
        rdfs:label "context16" ;
        n2mu:id "16"^^xml1:string ;
        sem:hasBeginTimeStamp leolaniContext:2022-12-14 ;
        sem:hasPlace leolaniContext:unknown16 .

    leolaniContext:contextNone sem:hasEvent leolaniTalk:chat84 .

    leolaniContext:2022-12-14 a sem:Time,
            time:DateTimeDescription ;
        rdfs:label "2022-12-14" ;
        time:day "14"^^xml1:gDay ;
        time:month "12"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2022"^^xml1:gYear .

    leolaniContext:unknown16 a n2mu:location,
            sem:Place ;
        rdfs:label "unknown" ;
        n2mu:id "84"^^xml1:string ;
        n2mu:in leolaniWorld: .

    leolaniTalk:chat84 a grasp:Chat,
            sem:Event ;
        rdfs:label "chat84" ;
        n2mu:id "84"^^xml1:string ;
        sem:hasSubEvent leolaniTalk:chat84_utterance1 .

    leolaniTalk:chat84_utterance1 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat84_utterance1" ;
        n2mu:id "1"^^xml1:string ;
        sem:hasActor leolaniWorld:speaker1 .

    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
        rdfs:label "speaker1" .
}

leolaniWorld:leolani_know_speaker1 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker1 .
}

leolaniWorld:Instances {
    leolaniWorld:class a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat84_utterance1_char0-50 .

    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:speaker1 a n2mu:person,
            gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat84_utterance1_char0-50 .
}

leolaniWorld:Claims {
    leolaniWorld:leolani_know_speaker1 a gaf:Assertion,
            sem:Event ;
        rdfs:label "leolani_know_speaker1" .

    leolaniWorld:speaker1_like_class a gaf:Assertion,
            sem:Event ;
        rdfs:label "speaker1_like_class" ;
        gaf:denotedBy leolaniTalk:chat84_utterance1_char0-50 .

    leolaniWorld:speaker1 owl:sameAs leolaniWorld:speaker1 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat84_utterance1_char0-50 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat84_utterance1_char0-50" ;
        gaf:containsDenotation leolaniWorld:class,
            leolaniWorld:speaker1 ;
        gaf:denotes leolaniWorld:speaker1_like_class ;
        grasp:hasAttribution leolaniTalk:speaker1_like_class_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker1 ;
        sem:hasBeginTimeStamp "2022-12-14 12:58:58.818308"^^xml1:string ;
        rdf:value " No problem , what class would you like to take ? "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat84_utterance1 .

    leolaniTalk:speaker1_like_class_0000 a grasp:Attribution ;
        rdfs:label "speaker1_like_class_0000" ;
        grasp:isAttributionFor leolaniTalk:chat84_utterance1_char0-50 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
        rdfs:label "UNDERSPECIFIED" .

    graspf:UNDERSPECIFIED a grasp:AttributionValue,
            graspf:CertaintyValue,
            graspf:PolarityValue ;
        rdfs:label "UNDERSPECIFIED" .

    grasps:UNDERSPECIFIED a grasp:AttributionValue,
            grasps:SentimentValue ;
        rdfs:label "UNDERSPECIFIED" .
}

