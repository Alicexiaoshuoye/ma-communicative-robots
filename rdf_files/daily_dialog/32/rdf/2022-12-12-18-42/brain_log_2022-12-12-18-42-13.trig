leolaniWorld:Instances {
    leolaniWorld:leolani a n2mu:robot ;
        rdfs:label "leolani" .

    leolaniWorld:material a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat103_utterance1_char0-108 .

    leolaniWorld:speaker1 a gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat103_utterance0_char0-17,
            leolaniTalk:chat103_utterance1_char0-108 .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance ;
        gaf:denotedIn leolaniTalk:chat103_utterance0_char0-17 .
}

leolaniTalk:Perspectives {
    leolaniTalk:chat103_utterance0_char0-17 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat103_utterance0_char0-17" ;
        gaf:containsDenotation leolaniWorld:speaker1,
            leolaniWorld:speaker2 ;
        gaf:denotes leolaniWorld:speaker2_help_speaker1 ;
        grasp:hasAttribution leolaniTalk:speaker2_help_speaker1_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:42:11.040545"^^xml1:string,
            "2022-12-12 18:42:11.697921"^^xml1:string,
            "2022-12-12 18:42:12.844002"^^xml1:string,
            "2022-12-12 18:42:13.927243"^^xml1:string ;
        rdf:value "Can I help you ? "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat103_utterance0 .

    leolaniTalk:chat103_utterance1_char0-108 a gaf:Mention,
            grasp:Statement ;
        rdfs:label "chat103_utterance1_char0-108" ;
        gaf:containsDenotation leolaniWorld:material,
            leolaniWorld:speaker1 ;
        gaf:denotes leolaniWorld:speaker1_look-for_material ;
        grasp:hasAttribution leolaniTalk:speaker1_look-for_material_0000 ;
        grasp:wasAttributedTo leolaniWorld:speaker2 ;
        sem:hasBeginTimeStamp "2022-12-12 18:42:12.143326"^^xml1:string,
            "2022-12-12 18:42:13.291142"^^xml1:string ;
        rdf:value " I hope so . I'm looking for some material for a paper I'm writing , and I'm not quite sure where to look . "^^xml1:string ;
        prov:wasDerivedFrom leolaniTalk:chat103_utterance1 .

    leolaniTalk:speaker1_look-for_material_0000 a grasp:Attribution ;
        rdfs:label "speaker1_look-for_material_0000" ;
        grasp:isAttributionFor leolaniTalk:chat103_utterance1_char0-108 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    leolaniTalk:speaker2_help_speaker1_0000 a grasp:Attribution ;
        rdfs:label "speaker2_help_speaker1_0000" ;
        grasp:isAttributionFor leolaniTalk:chat103_utterance0_char0-17 ;
        rdf:value graspe:UNDERSPECIFIED,
            graspf:UNDERSPECIFIED,
            grasps:UNDERSPECIFIED .

    graspe:UNDERSPECIFIED a grasp:AttributionValue,
            graspe:EmotionValue ;
