leolaniWorld:leolani_know_speaker2 {
    leolaniWorld:leolani n2mu:know leolaniWorld:speaker2 .
}

leolaniWorld:speaker2_help_speaker1 {
    leolaniWorld:speaker2 n2mu:help leolaniWorld:speaker1 .
}

