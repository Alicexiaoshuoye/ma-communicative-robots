        sem:hasSubEvent leolaniTalk:chat103_utterance0,
            leolaniTalk:chat103_utterance1 .

    leolaniTalk:chat103_utterance0 a grasp:Utterance,
            sem:Event ;
        rdfs:label "chat103_utterance0" ;
        n2mu:id "0"^^xml1:string ;
