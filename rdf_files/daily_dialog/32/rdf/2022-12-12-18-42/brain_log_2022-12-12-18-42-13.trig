    leolaniWorld: a n2mu:city,
            n2mu:country,
            n2mu:location,
            n2mu:region,
            sem:Place ;
        rdfs:label "" .

    leolaniWorld:speaker2 a n2mu:person,
            gaf:Instance,
            grasp:Source,
            sem:Actor ;
